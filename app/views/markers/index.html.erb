<% content_for :head do %>
	<%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyBhOAgUCn6GQzKlnMwFmO2kBvG_S3JRD38&libraries=places&sensor=false" %>
<% end %>
<div id="map-container">
	<div id="map-canvas"/>
</div>
<div class="info-content">
      <table>
        <tr id="iw-url-row" class="iw_table_row">
          <td id="iw-icon" class="iw_table_icon"></td>
          <td id="iw-url"></td>
        </tr>
        <tr id="iw-description-row" class="iw_table_row">
          <td class="iw_attribute_name">Rating:</td>
          <td id="iw-description"></td>
        </tr>
        <tr id="iw-title-row" class="iw_table_row">
          <td class="iw_attribute_name">Website:</td>
          <td id="iw-title"></td>
        </tr>
      </table>
</div>

<script type="text/javascript">
	var markers = <%= @markers %>
	$(document).ready(function(){
		initialize();
	});
</script>